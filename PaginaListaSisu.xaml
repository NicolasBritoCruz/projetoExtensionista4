<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="Extensionista.PaginaListaSisu"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="PaginaListaSisu">
    <AbsoluteLayout BackgroundColor="#ffffff">
        <ImageButton
            x:Name="sairListaSisu"
            AbsoluteLayout.LayoutBounds="0.94, 0.044"
            AbsoluteLayout.LayoutFlags="PositionProportional"
            Clicked="sairListaSisu_Clicked"
            HeightRequest="35"
            Source="x.png" />
        <Label
            AbsoluteLayout.LayoutBounds="0.5, 0.045"
            AbsoluteLayout.LayoutFlags="PositionProportional"
            FontFamily="MontserratSemiBold"
            FontSize="22"
            Text="Universidades ofertantes"
            TextColor="#245897" />
        <Button
            x:Name="btnCursosSisu"
            AbsoluteLayout.LayoutBounds="0.5, 0.8"
            AbsoluteLayout.LayoutFlags="PositionProportional"
            Clicked="btnCursosSisu_Clicked"
            Text="cursos" />
    </AbsoluteLayout>
</ContentPage>